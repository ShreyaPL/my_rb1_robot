<?xml version="1.0" ?>
<robot name="rb1_robot">

    <!-- Gazebo plugin for differential drive controller -->

    <!-- Differential Drive Plugin -->
    <gazebo>
        <plugin
            filename="libgazebo_ros_diff_drive.so"
            name="differential_drive_controller">
            <alwaysOn>true</alwaysOn>
            <updateRate>20</updateRate>
            <leftJoint>base_link_left_wheel_joint</leftJoint>
            <rightJoint>base_link_right_wheel_joint</rightJoint>

            <publishWheelTF>true</publishWheelTF>
            <publishOdom>true</publishOdom>
            <publishTf>true</publishTf>
            <publishWheelJointState>true</publishWheelJointState>

            <wheelSeparation>0.4</wheelSeparation>
            <wheelDiameter>0.05</wheelDiameter>
            <torque>10</torque>
            
            <commandTopic>cmd_vel</commandTopic>
            <odometryTopic>odom</odometryTopic>
            <odometryFrame>odom</odometryFrame>
            <robotBaseFrame>base_footprint</robotBaseFrame>
        </plugin>
    </gazebo>

    <!-- Gazebo configuration for the front laser sensor -->
    <gazebo reference="front_laser">
        <!-- Define a ray sensor named "head_hokuyo_sensor" -->
        <sensor type="ray" name="head_hokuyo_sensor">
            <pose>0 0 0 0 0 0</pose>
            <!-- Enable visualization of the sensor -->
            <visualize>false</visualize>
            <update_rate>20</update_rate>
            <ray>
                <!-- Configure the scanning properties -->
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.570796</min_angle>
                        <max_angle>1.570796</max_angle>
                    </horizontal>
                </scan>
                <!-- Configure the range properties -->
                <range>
                    <min>0.05</min>
                    <max>10.0</max>
                    <resolution>0.01</resolution>
                </range>
                <!-- Configure the noise properties -->
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
            <!-- Attach a Gazebo plugin to the sensor for ROS integration -->
            <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
                <topicName>/scan</topicName>
                <frameName>front_laser</frameName>
            </plugin>
        </sensor>
    </gazebo>

    <!-- Specify Gazebo color for the base_link -->
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- Gazebo friction and material properties for left_wheel -->
    <gazebo reference="left_wheel">
        <mu>5.0</mu>
        <mu2>0.5</mu2>
        <kp>10000000.0</kp>
        <kd>1.0</kd>
        <fdir1>1 0 0</fdir1>
        <material>Gazebo/Red</material>
    </gazebo>

    <!-- Gazebo friction and material properties for right_wheel -->
    <gazebo reference="right_wheel">
        <mu>5.0</mu>
        <mu2>0.5</mu2>
        <kp>10000000.0</kp>
        <kd>1.0</kd>
        <fdir1>1 0 0</fdir1>
        <material>Gazebo/Red</material>
    </gazebo>

    <!-- Gazebo friction and material properties for front_caster -->
    <gazebo reference="front_caster">
        <mu>0.0</mu>
        <mu2>0.0</mu2>
        <kp>10000000.0</kp>
        <kd>100.0</kd>
        <fdir1>0 0 1</fdir1>
        <material>Gazebo/Gray</material>
    </gazebo>

    <!-- Gazebo friction and material properties for back_caster -->
    <gazebo reference="back_caster">
        <mu>0.0</mu>
        <mu2>0.0</mu2>
        <kp>10000000.0</kp>
        <kd>100.0</kd>
        <fdir1>0 0 1</fdir1>
        <material>Gazebo/Gray</material>
    </gazebo>

    <!-- ============ Materials ============ -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>
    <material name="gray">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- ============ Base footprint ============ -->
    <link name="base_footprint">
    </link>

    <!-- base_footprint -> base_link  -->
    <joint name="base_link_plate_joint" type="fixed">
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
        <parent link="base_footprint"/>
        <child  link="base_link"/>
    </joint>

    <!-- ============ Base link ============ -->
    <link name="base_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="20.5"/>
            <inertia ixx="0.47406249999999994" ixy="0.0" ixz="0.0" iyy="0.47406249999999994" iyz="0" izz="0.640625"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <!-- ============ Left wheel ============ -->
    <link name="left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="-1.575 0 0"/>
            <mass value="1.5"/>
            <inertia ixx="0.00034687500000000007" ixy="0.0" ixz="0.0"
                     iyy="0.00034687500000000007" iyz="0.0" izz="0.0004687500000000001"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="-1.575 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
        </collision>
        <visual>
            <!-- rotate so cylinder axis is Y -->
            <origin xyz="0 0 0" rpy="-1.575 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
            <material name="red"/>
        </visual>
    </link>

    <!-- base_link -> left_wheel -->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="left_wheel"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
    </joint>

    <!-- ============ Right wheel x============ -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="-1.575 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="-1.575 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="-1.575 0 0"/>
            <mass value="1.5"/>
            <inertia ixx="0.00034687500000000007" ixy="0" ixz="0"
                     iyy="0.00034687500000000007" iyz="0" izz="0.0004687500000000001"/>
        </inertial>
    </link>

    <!-- base_link -> right_wheel -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="right_wheel"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
    </joint>

    <!-- ============ Front caster ============ -->
    <link name="front_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <contact_coefficients mu="0.0" kp="0.0" kd="0.0" />
            <geometry><sphere radius="0.025"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.00012500000000000003" ixy="0" ixz="0"
                     iyy="0.00012500000000000003" iyz="0" izz="0.00012500000000000003"/>
        </inertial>
    </link>

    <joint name="base_link_front_caster_joint" type="fixed">
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="front_caster"/>
    </joint>

    <!-- ============ Back caster ============ -->
    <link name="back_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <contact_coefficients mu="0.0" kp="0.0" kd="0.0" />
            <geometry><sphere radius="0.025"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.00012500000000000003" ixy="0" ixz="0"
                     iyy="0.00012500000000000003" iyz="0" izz="0.00012500000000000003"/>
        </inertial>
    </link>

    <joint name="base_link_back_caster_joint" type="fixed">
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="back_caster"/>
    </joint>

    <!-- ============ Front laser link  ============ -->
    <link name="front_laser">
    </link>

    <joint name="base_link_front_laser_joint" type="fixed">
        <origin xyz="0.25 0.0 0.075" rpy="3.14 0 0"/>
        <parent link="base_link"/>
        <child  link="front_laser"/>
    </joint>
</robot>