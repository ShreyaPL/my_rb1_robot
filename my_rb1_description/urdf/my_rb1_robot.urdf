<?xml version="1.0" ?>
<robot name="rb1_robot">

    <!-- Differential Drive Plugin -->
    <gazebo>
        <plugin
            filename="libignition-gazebo-diff-drive-system.so"
            name="ignition::gazebo::systems::DiffDrive">
            <left_joint>base_link_left_wheel_joint</left_joint>
            <right_joint>base_link_right_wheel_joint</right_joint>
            <wheel_separation>0.40</wheel_separation>
            <wheel_radius>0.025</wheel_radius>
            <odom_publish_frequency>20</odom_publish_frequency>
            <topic>/cmd_vel</topic>
            <odom_topic>odom</odom_topic>
            <tf_topic>tf</tf_topic>
            <frame_id>odom</frame_id>
            <child_frame_id>base_footprint</child_frame_id>
        </plugin>
    </gazebo>

    <!-- Laser Scanner Plugin -->
    <gazebo>
        <plugin
        filename="libignition-gazebo-sensors-system.so"
        name="ignition::gazebo::systems::Sensors">
        <!-- use ogre2 if ogre v2.x is installed, otherwise use ogre -->
        <render_engine>ogre2</render_engine>
        </plugin>
    </gazebo>
    <gazebo reference="front_laser">
        <sensor type="gpu_lidar" name="head_hokuyo_sensor">
            <pose relative_to='lidar_frame'>0 0 0 0 0 0</pose>
            <update_rate>20</update_rate>
            <topic>/scan</topic>
            <always_on>1</always_on>
            <visualize>true</visualize>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.57096</min_angle>
                        <max_angle>1.57096</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.20</min>
                    <max>10.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
        </sensor>
    </gazebo>

    <!-- Specify Gazebo color for the base_link -->
    <gazebo reference="base_link">
        <visual>
            <material> <!-- Black -->
                <ambient>0 0 0 1</ambient>
                <diffuse>0 0 0 1</diffuse>
                <specular>0 0 0 1</specular>
                <emissive>0 0 0 1</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Gazebo friction and material properties for left_wheel -->
    <gazebo reference="left_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>5.0</mu>
                        <mu2>0.5</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Gazebo friction and material properties for right_wheel -->
    <gazebo reference="right_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>5.0</mu>
                        <mu2>0.5</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Gazebo friction and material properties for front_caster -->
    <gazebo reference="front_caster">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>5.0</mu>
                        <mu2>0.5</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Gazebo friction and material properties for back_caster -->
    <gazebo reference="back_caster">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>5.0</mu>
                        <mu2>0.5</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- ============ Materials ============ -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>
    <material name="gray">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- ============ Base footprint ============ -->
    <link name="base_footprint">
    </link>

    <!-- base_footprint -> base_link  -->
    <joint name="base_link_plate_joint" type="fixed">
        <origin xyz="0 0 0.175" rpy="0 0 0"/>
        <parent link="base_footprint"/>
        <child  link="base_link"/>
    </joint>

    <!-- ============ Base link ============ -->
    <link name="base_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="22.0"/>
            <inertia ixx="0.50875" ixy="0.0" ixz="0.0" iyy="0.50875" iyz="0" izz="0.6875"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <!-- ============ Left wheel ============ -->
    <link name="left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="9.4791667e-05" ixy="0.0" ixz="0.0"
                     iyy="9.4791667e-05" iyz="0.0" izz="1.5625e-04"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
        </collision>
        <visual>
            <!-- rotate so cylinder axis is Y -->
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
            <material name="red"/>
        </visual>
    </link>

    <!-- base_link -> left_wheel -->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="left_wheel"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- ============ Right wheel x============ -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="9.4791667e-05" ixy="0" ixz="0"
                     iyy="9.4791667e-05" iyz="0" izz="1.5625e-04"/>
        </inertial>
    </link>

    <!-- base_link -> right_wheel -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="right_wheel"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- ============ Front caster ============ -->
    <link name="front_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.3"/>
            <inertia ixx="7.5e-05" ixy="0" ixz="0"
                     iyy="7.5e-05" iyz="0" izz="7.5e-05"/>
        </inertial>
    </link>

    <joint name="base_link_front_caster_joint" type="fixed">
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="front_caster"/>
    </joint>

    <!-- ============ Back caster ============ -->
    <link name="back_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.3"/>
            <inertia ixx="7.5e-05" ixy="0" ixz="0"
                     iyy="7.5e-05" iyz="0" izz="7.5e-05"/>
        </inertial>
    </link>

    <joint name="base_link_back_caster_joint" type="fixed">
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="back_caster"/>
    </joint>

    <!-- ============ Front laser link  ============ -->
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.4"/>
            <inertia ixx="0.0011666667" ixy="0" ixz="0"
                     iyy="0.0011666667" iyz="0" izz="0.00084"/>
        </inertial>
    </link>

    <joint name="base_link_front_laser_joint" type="fixed">
        <origin xyz="0.25 0.0 0.075" rpy="3.1416 0 0"/>
        <parent link="base_link"/>
        <child  link="front_laser"/>
    </joint>
</robot>