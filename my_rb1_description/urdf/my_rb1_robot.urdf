<?xml version="1.0" ?>
<robot name="rb1_robot">

    <!-- ============ Materials ============ -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>
    <material name="gray">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- ============ Base footprint ============ -->
    <link name="base_footprint">
    </link>

    <!-- base_footprint -> base_link  -->
    <joint name="base_link_plate_joint" type="fixed">
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
        <parent link="base_footprint"/>
        <child  link="base_link"/>
    </joint>

    <!-- ============ Base link ============ -->
    <link name="base_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="22.0"/>
            <inertia ixx="0.50875" ixy="0" ixz="0" iyy="0.50875" iyz="0" izz="0.6875"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><cylinder radius="0.25" length="0.30"/></geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <!-- ============ Left wheel ============ -->
    <link name="left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="9.4791667e-05" ixy="0" ixz="0"
                     iyy="9.4791667e-05" iyz="0" izz="1.5625e-04"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry><cylinder radius="0.025" length="0.02"/></geometry>
        </collision>
        <visual>
            <!-- rotate so cylinder axis is Y -->
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry><cylinder radius="0.025" length="0.02"/></geometry>
            <material name="gray"/>
        </visual>
    </link>

    <!-- base_link -> left_wheel -->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="left_wheel"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- ============ Right wheel x============ -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry><cylinder radius="0.025" length="0.02"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry><cylinder radius="0.025" length="0.02"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="9.4791667e-05" ixy="0" ixz="0"
                     iyy="9.4791667e-05" iyz="0" izz="1.5625e-04"/>
        </inertial>
    </link>

    <!-- base_link -> right_wheel -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="right_wheel"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- ============ Front caster ============ -->
    <link name="front_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.3"/>
            <inertia ixx="7.5e-05" ixy="0" ixz="0"
                     iyy="7.5e-05" iyz="0" izz="7.5e-05"/>
        </inertial>
    </link>

    <joint name="base_link_front_caster_joint" type="fixed">
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="front_caster"/>
    </joint>

    <!-- ============ Back caster ============ -->
    <link name="back_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry><sphere radius="0.025"/></geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.3"/>
            <inertia ixx="7.5e-05" ixy="0" ixz="0"
                     iyy="7.5e-05" iyz="0" izz="7.5e-05"/>
        </inertial>
    </link>

    <joint name="base_link_back_caster_joint" type="fixed">
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child  link="back_caster"/>
    </joint>

    <!-- Gazebo friction overrides for casters -->
    <!-- <gazebo reference="front_caster"><mu1>0.0</mu1><mu2>0.0</mu2></gazebo>
    <gazebo reference="back_caster"><mu1>0.0</mu1><mu2>0.0</mu2></gazebo> -->

    <!-- ============ Front laser link  ============ -->
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.4"/>
            <inertia ixx="0.0011666667" ixy="0" ixz="0"
                     iyy="0.0011666667" iyz="0" izz="0.00084"/>
        </inertial>
    </link>

    <joint name="base_link_front_laser_joint" type="fixed">
        <origin xyz="0.25 0.0 0.075" rpy="3.1416 0 0"/>
        <parent link="base_link"/>
        <child  link="front_laser"/>
    </joint>

</robot>
